
    <% @bookings.each do |booking| %>
    <%= cl_image_tag booking.boat.image.key, height: 180, width: 320, crop: :fill,position: :center %>
    <%= booking.checkin %>
    <%= booking.checkout %>
    <%= link_to "Delete",booking_path(booking),data: {turbo_method: "delete"}, class: "lrg-blue-btn" %>
    <% end %>
